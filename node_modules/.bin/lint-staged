#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/gilsozinho/workspace/github.com/JokersFiasco99/neumorph-maker-pro/node_modules/.pnpm/lint-staged@14.0.1/node_modules/lint-staged/bin/node_modules:/Users/gilsozinho/workspace/github.com/JokersFiasco99/neumorph-maker-pro/node_modules/.pnpm/lint-staged@14.0.1/node_modules/lint-staged/node_modules:/Users/gilsozinho/workspace/github.com/JokersFiasco99/neumorph-maker-pro/node_modules/.pnpm/lint-staged@14.0.1/node_modules:/Users/gilsozinho/workspace/github.com/JokersFiasco99/neumorph-maker-pro/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/gilsozinho/workspace/github.com/JokersFiasco99/neumorph-maker-pro/node_modules/.pnpm/lint-staged@14.0.1/node_modules/lint-staged/bin/node_modules:/Users/gilsozinho/workspace/github.com/JokersFiasco99/neumorph-maker-pro/node_modules/.pnpm/lint-staged@14.0.1/node_modules/lint-staged/node_modules:/Users/gilsozinho/workspace/github.com/JokersFiasco99/neumorph-maker-pro/node_modules/.pnpm/lint-staged@14.0.1/node_modules:/Users/gilsozinho/workspace/github.com/JokersFiasco99/neumorph-maker-pro/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/lint-staged@14.0.1/node_modules/lint-staged/bin/lint-staged.js" "$@"
else
  exec node  "$basedir/../.pnpm/lint-staged@14.0.1/node_modules/lint-staged/bin/lint-staged.js" "$@"
fi
